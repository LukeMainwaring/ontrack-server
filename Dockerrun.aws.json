{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "server",
      "image": "lukemain/ontrack-server",
      "hostname": "ontrack-server",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "memory": 128
    }
  ]
}
